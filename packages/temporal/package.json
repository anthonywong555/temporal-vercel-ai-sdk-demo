{
  "name": "@temporal-vercel-demo/temporalio",
  "version": "0.0.1",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./dist node_modules .turbo",
    "dev": "npm run build --watch",
    "build": "tsup index.ts base/index.ts --format cjs,esm --dts --outDir dist",
    "build.watch": "npm run build --watch",
    "lint": "TIMING=1 eslint \"**/*.ts*\""
  },
  "dependencies": {
    "@temporal-vercel-demo/common": "workspace:*",
    "@temporalio/client": "catalog:"
  },
  "devDependencies": {
    "@temporal-vercel-demo/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "tsup": "catalog:",
    "@temporalio/interceptors-opentelemetry": "catalog:",
    "typescript": "catalog:"
  }
}
