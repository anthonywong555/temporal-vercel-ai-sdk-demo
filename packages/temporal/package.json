  "dependencies": {
    "@boilerplate/common": "workspace:*",
    "@temporalio/common": "catalog:",
    "@temporalio/proto": "catalog:",
    "@temporalio/client": "catalog:"
  },
  "devDependencies": {
    "@boilerplate/tsconfig": "workspace:*",
    "eslint": "^9.33.0",
    "tsup": "^8.4.0",
    "@temporalio/interceptors-opentelemetry": "catalog:",
    "typescript": "^5.6.3"
  }
}
